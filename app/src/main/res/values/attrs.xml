<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwipyRefreshLayout">
        <attr name="direction">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="both" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FAMLayout">
        <attr name="fam_radius" format="dimension"/>
        <attr name="fam_animation">
            <enum name="order" value="0"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CommonSearchLayout">
        <attr name="left_image" format="reference"/>
        <attr name="cancel_image" format="reference"/>
        <attr name="edit_text_hint" format="reference|string"/>
    </declare-styleable>
    
    <declare-styleable name="AutoHorizontalLinearLayout">
        <attr name="space" format="dimension|integer"/>
    </declare-styleable>

    <declare-styleable name="CursorView">
        <attr name="cursor_count" format="reference|integer"/>
    </declare-styleable>

</resources>